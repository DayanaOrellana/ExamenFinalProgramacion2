<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="edu.umg.InscripcionesDTO">
        <table name="inscripciones" schema="public" catalog="postgres"/>
        <attributes>
            <id name="idInscripcion">
                <column name="id_inscripcion" precision="-1"/>
            </id>
            <basic name="idEstudiante">
                <column name="id_estudiante" nullable="false" precision="-1"/>
            </basic>
            <basic name="idCurso">
                <column name="id_curso" nullable="false" precision="-1"/>
            </basic>
            <basic name="fechaInscripcion">
                <column name="fecha_inscripcion" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
